<!DOCTYPE html>
<html lang="en" dir="ltr">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>@ViewBag.Title - Seratio Platform </title>
    <link href="/areas/L0C5A3E8915871B710C2CC98073748424/assets/images/favicon.png" rel="icon" type="image/png">
    <link href="https://fonts.googleapis.com/css?family=Roboto:400,300,100,500,700,900" rel="stylesheet" type="text/css">

    <link href="/areas/L0C5A3E8915871B710C2CC98073748424/assets/css/bootstrap.css" rel="stylesheet" type="text/css">
    <link href="/areas/L0C5A3E8915871B710C2CC98073748424/assets/css/icons/icomoon/styles.css" rel="stylesheet" type="text/css">
    <link href="/areas/L0C5A3E8915871B710C2CC98073748424/assets/css/styles.css?_=@AppManager.Now.Ticks" rel="stylesheet" type="text/css">
    <link href="/areas/L0C5A3E8915871B710C2CC98073748424/assets/css/theme.css?_=@AppManager.Now.Ticks" rel="stylesheet" type="text/css">
    <link href="/areas/L0C5A3E8915871B710C2CC98073748424/assets/css/select2.css" rel="stylesheet" type="text/css">
    <link href="~/areas/L0C5A3E8915871B710C2CC98073748424/assets/css/jquery.tagit.css" rel="stylesheet" type="text/css">
    <link href="~/areas/L0C5A3E8915871B710C2CC98073748424/assets/css/tagit.ui-zendesk.css" rel="stylesheet" type="text/css">
    <link href="/areas/L0C5A3E8915871B710C2CC98073748424/assets/plugins/bootstrap-toastr/toastr.min.css" rel="stylesheet" type="text/css" />
    <link href="/areas/L0C5A3E8915871B710C2CC98073748424/assets/plugins/bootstrap-daterangepicker/daterangepicker-bs3.css" rel="stylesheet" type="text/css" />
    <link rel="stylesheet" href="//cdnjs.cloudflare.com/ajax/libs/timepicker/1.3.5/jquery.timepicker.min.css">
    @RenderSection("CSS", required: false)
    <!--[if lt IE 9]>
      <script src="https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script>
      <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->
</head>
<body>
    <div class="navbar navbar-inverse">
        <div class="navbar-header">
            <a class="navbar-brand" href="/L0C5A3E8915871B710C2CC98073748424/default/dashboard"><img src="~/areas/L0C5A3E8915871B710C2CC98073748424/assets/images/logo-small.png" alt="Seratio Platform"></a>
            <ul class="nav navbar-nav pull-right visible-xs-block">
                <li><a data-toggle="collapse" data-target="#navbar-mobile"><i class="icon-tree5"></i></a></li>
                <li><a class="sidebar-mobile-main-toggle"><i class="icon-paragraph-justify3"></i></a></li>
            </ul>
        </div>
        <div class="navbar-collapse collapse" id="navbar-mobile">
            <ul class="nav navbar-nav navbar-right">
                <li class="dropdown dropdown-user">
                    <a class="dropdown-toggle" data-toggle="dropdown">
                        @{
                            if (AppManager.Administrator != null)
                            {
                                @*if (!string.IsNullOrEmpty(AppManager.User.Photo))
                                    {
                                        string _path = HttpContext.Current.Server.MapPath(@"~/images/users/" + (AppManager.User.Photo));

                                        FileInfo _file = new FileInfo(_path);

                                        if (!_file.Exists)
                                        {
                                            <img src="~/areas/L0C5A3E8915871B710C2CC98073748424/assets/images/avatar.png" class="img-circle" alt="">
                                        }
                                        else
                                        {
                                            <img src="~/Images/users/@(AppManager.User.Photo)" class="img-circle" alt="">
                                        }
                                    }
                                    else
                                    {
                                        <img src="~/areas/L0C5A3E8915871B710C2CC98073748424/assets/images/avatar.png" class="img-circle" alt="">
                                    }*@
                                <img src="~/areas/L0C5A3E8915871B710C2CC98073748424/assets/images/avatar.png" class="img-circle" alt="">
                                @(AppManager.Administrator.Name)
                            }
                        }

                        <i class="caret"></i>
                    </a>
                    <ul class="dropdown-menu dropdown-menu-right">
                        @{
                            if (AppManager.Administrator != null)
                            {
                                <li><a href="/L0C5A3E8915871B710C2CC98073748424/administrators/details/@(AppManager.Administrator.ID)"><i class="icon-user-plus"></i> My profile</a></li>
                                <li><a href="/L0C5A3E8915871B710C2CC98073748424/administrators/changepassword/@(AppManager.Administrator.ID)"><i class="icon-lock2"></i> Change Password</a></li>
                            }
                        }
                        <li class="divider"></li>
                        <li><a href="/L0C5A3E8915871B710C2CC98073748424/default/logout"><i class="icon-switch2"></i> Logout</a></li>
                    </ul>
                </li>
            </ul>
        </div>
    </div>
    <div class="page-container">
        <div class="page-content">
            <div class="sidebar sidebar-main">
                <div class="sidebar-content">
                    <div class="sidebar-category sidebar-category-visible">
                        <div class="category-content no-padding">
                            <ul class="navigation navigation-main navigation-accordion main-menu">
                                <li>
                                    <a href="~/L0C5A3E8915871B710C2CC98073748424/default/dashboard"><i class="icon-home4"></i> <span>Dashboard</span></a>
                                </li>
                                <li>
                                    <a href="/L0C5A3E8915871B710C2CC98073748424/users"><i class="icon-users"></i> <span>Users</span></a>
                                </li>
                                @*<li>
                                    <a href="/L0C5A3E8915871B710C2CC98073748424/offers"><i class="icon-price-tag"></i> <span>Offers</span></a>
                                </li>
                                <li>
                                    <a href="/L0C5A3E8915871B710C2CC98073748424/wallets"><i class="icon-wallet"></i> <span>Wallets</span></a>
                                </li>*@
                                <li>
                                    <a href="/L0C5A3E8915871B710C2CC98073748424/investments"><i class="icon-wallet"></i> <span>Investments</span></a>
                                </li>
                                @*<li>
                                    <a href="/L0C5A3E8915871B710C2CC98073748424/wallets"><i class="icon-wallet"></i> <span>Wallets</span></a>
                                </li>*@
                                <li>
                                    <a href="javascript:void(0)"><i class="icon-sphere"></i> <span>Settings</span></a>
                                    <ul>
                                      
                                        <li>
                                            <a href="/L0C5A3E8915871B710C2CC98073748424/administrators"><i class="icon-user"></i> <span>Administrators</span></a>
                                        </li>

                                        @{
                                            bool _automaticSync = AppManager.db.Settings.Any() ? AppManager.db.Settings.Select(i => i.AllowAutomaticExchangeRateSyncing).FirstOrDefault() : true;

                                            if (_automaticSync == false)
                                            {
                                                <li>
                                                    <a href="/L0C5A3E8915871B710C2CC98073748424/settings/update"><i class="icon-coins"></i> <span>Update Exchange Fiats</span></a>
                                                </li>
                                            }

                                        }
                                        <li>
                                            <a href="/L0C5A3E8915871B710C2CC98073748424/settings"><i class="icon-wrench3"></i> <span>General Settings</span></a>
                                        </li>
                                        <li>
                                            <a href="/L0C5A3E8915871B710C2CC98073748424/extras/walletinfo"><i class="icon-wrench3"></i> <span>Wallet Info. (Source)</span></a>
                                        </li>
                                    </ul>
                                </li>
                            </ul>
                        </div>
                    </div>
                </div>
            </div>
            <div class="content-wrapper">
                @RenderBody()
                <div class="footer">© 2017 CCEG. All Rights Reserved.</div>
            </div>
        </div>
    </div>

    <script type="text/javascript" src="/areas/L0C5A3E8915871B710C2CC98073748424/assets/js/jquery.min.js"></script>
    <script type="text/javascript" src="/areas/L0C5A3E8915871B710C2CC98073748424/assets/js/bootstrap.min.js"></script>
    <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/moment.js/2.14.1/moment.min.js"></script>
    <script type="text/javascript" src="~/areas/L0C5A3E8915871B710C2CC98073748424/assets/js/select2.js"></script>
    <script type="text/javascript" src="//cdnjs.cloudflare.com/ajax/libs/fullcalendar/2.9.1/fullcalendar.min.js"></script>
    <script type="text/javascript" src="/areas/L0C5A3E8915871B710C2CC98073748424/assets/plugins/pickers/pickadate/picker.js"></script>
    <script type="text/javascript" src="/areas/L0C5A3E8915871B710C2CC98073748424/assets/plugins/pickers/pickadate/picker.date.js"></script>
    <script type="text/javascript" src="/areas/L0C5A3E8915871B710C2CC98073748424/assets/plugins/forms/styling/uniform.min.js"></script>
    <script type="text/javascript" src="/areas/L0C5A3E8915871B710C2CC98073748424/assets/plugins/tables/datatables/datatables.min.js"></script>
    <script src="~/areas/L0C5A3E8915871B710C2CC98073748424/assets/js/app.js" type="text/javascript"></script>
    <script src="~/areas/L0C5A3E8915871B710C2CC98073748424/assets/js/jquery.livequery.js" type="text/javascript"></script>
    <script src="~/Scripts/jquery.validate.min.js" type="text/javascript"></script>
    <script src="http://cdn.jsdelivr.net/jquery.validation/1.15.0/additional-methods.min.js"></script>
    <script src="~/areas/L0C5A3E8915871B710C2CC98073748424/assets/plugins/bootstrap-toastr/toastr.min.js" type="text/javascript"></script>
    <script src="~/areas/L0C5A3E8915871B710C2CC98073748424/assets/plugins/pickers/daterangepicker.js"></script>
    <script src="~/areas/L0C5A3E8915871B710C2CC98073748424/assets/plugins/pickers/datepicker.js" type="text/javascript"></script>
    <script src="/areas/L0C5A3E8915871B710C2CC98073748424/assets/js/jquery.cookie.min.js" type="text/javascript"></script>
    <script type="text/javascript" src="/areas/L0C5A3E8915871B710C2CC98073748424/assets/js/scripts.js"></script>
    <script src="~/Scripts/jquery.validate.unobtrusive.js"></script>
    @RenderSection("JavaScript", required: false)
    <script src="~/Scripts/app.js?_=@AppManager.Now.Ticks" type="text/javascript"></script>
    <script>
        $(document).ready(function () {
            App.init();
            @{
                if (TempData["Notification"] != null)
                {
                    Notification notification = (Notification)TempData["Notification"];
                    <text>WebApp.Notifications.Show('@notification.Type', '@notification.Text');</text>
                }
             }
        });
    </script>
</body>
</html>