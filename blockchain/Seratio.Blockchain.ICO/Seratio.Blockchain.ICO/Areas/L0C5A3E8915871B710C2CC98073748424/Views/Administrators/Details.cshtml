@model Administrator

@{
    ViewBag.Title = "Administrator Details";
    Layout = "~/Areas/L0C5A3E8915871B710C2CC98073748424/Views/Shared/_Layout.cshtml";
}
@section JavaScript{
    <script type="text/javascript">$(document).ready(function () { WebApp.Core.HighlightCurrentMenuItem('Administrators') });</script>
    <script src="~/areas/L0C5A3E8915871B710C2CC98073748424/Assets/plugins/bootstrap-toggle/bootstrap-toggle.min.js"></script>
    <script type="text/javascript">
        $('a[data-toggle="tab"]').on('shown.bs.tab', function (e) {
            $($.fn.dataTable.tables(true)).DataTable()
               .columns.adjust();
        });
    </script>
}
@section CSS{
    <link href="~/areas/L0C5A3E8915871B710C2CC98073748424/Assets/plugins/bootstrap-toggle/bootstrap-toggle.min.css" rel="stylesheet" />
}

<div class="page-header">
    <div class="page-header-content">
        <div class="page-title">
            <h4><span class="text-semibold">Administrator Details</span></h4>
        </div>
        <div class="heading-elements">
            <div class="heading-btn-group">
                <a class="btn btn-link btn-float has-text" href="/L0C5A3E8915871B710C2CC98073748424/administrators/edit/@Model.ID"><i class="icon-pencil7 text-primary"></i><span>Edit</span></a>
                <a class="btn btn-link btn-float has-text confirm mr5" title="Delete" data-confirm="Are you sure you want to delete this Administrator?" href="/L0C5A3E8915871B710C2CC98073748424/administrators/delete/@Model.ID"><i class="icon-cancel-square text-primary"></i><span>Delete</span></a>
            </div>
        </div>
    </div>
    <div class="breadcrumb-line">
        <ul class="breadcrumb">
            <li><a href="/L0C5A3E8915871B710C2CC98073748424/default/dashboard"><i class="icon-home4 position-left"></i>Dashboard</a></li>
            <li><a href="/L0C5A3E8915871B710C2CC98073748424/administrators/index"> Administrators</a></li>
            <li class="active">Administrator Details</li>
        </ul>
    </div>
</div>
<div class="content">
    <div class="row">
        <div class="col-lg-12">
            <div class="panel panel-flat">
                <div class="panel-body">
                    <ul class="nav nav-tabs" role="tablist">
                        <li role="presentation" class="active"><a href="#general" aria-controls="general" role="tab" data-toggle="tab">General</a></li>
                        <li role="presentation"><a href="#history" aria-controls="history" role="tab" data-toggle="tab">Login History</a></li>
                    </ul>
                    <div class="tab-content">
                        <div role="tabpanel" class="tab-pane fade in active" id="general">
                            <div class="main">
                                <div class="col-md-12">
                                    <table class="table table-borderless table-xs content-group-sm">
                                        <tbody>

                                            <tr>
                                                <td class="col-lg-2">@Html.DisplayNameFor(model => model.Name)</td>
                                                <td class="col-lg-1">:</td>
                                                <td> @Model.Name</td>
                                            </tr>
                                            <tr>
                                                <td class="col-lg-2">@Html.DisplayNameFor(model => model.Email)</td>
                                                <td class="col-lg-1">:</td>
                                                <td> @Model.Email</td>
                                            </tr>
                                            <tr>
                                                <td class="col-lg-2">@Html.DisplayNameFor(model => model.Phone)</td>
                                                <td class="col-lg-1">:</td>
                                                <td> @Model.Phone</td>
                                            </tr>
                                            <tr>
                                                <td class="col-lg-2">@Html.DisplayNameFor(model => model.IsActive)</td>
                                                <td class="col-lg-1">:</td>
                                                <td>@Html.EditorFor(model => model.IsActive, new { htmlAttributes = new { @class = "toggle-switch", @disabled = "disabled" } })</td>
                                            </tr>
                                            <tr>
                                                <td class="col-lg-2">@Html.DisplayNameFor(model => model.IsSuperAdmin)</td>
                                                <td class="col-lg-1">:</td>
                                                <td>@Html.EditorFor(model => model.IsSuperAdmin, new { htmlAttributes = new { @class = "toggle-switch", @disabled = "disabled" } })</td>
                                            </tr>
                                            <tr>
                                                <td class="col-lg-2">@Html.DisplayNameFor(model => model.CreatedOn)</td>
                                                <td class="col-lg-1">:</td>
                                                <td> @Model.CreatedOn</td>
                                            </tr>
                                            <tr>
                                                <td class="col-lg-2">@Html.DisplayNameFor(model => model.LastUpdatedOn)</td>
                                                <td class="col-lg-1">:</td>
                                                <td> @Model.LastUpdatedOn</td>
                                            </tr>
                                        </tbody>
                                    </table>
                                </div>
                            </div>
                        </div>
                        <div role="tabpanel" class="tab-pane fade in" id="history">
                            <table class="table data-table table-bordered table-striped table-hover" data-source="/L0C5A3E8915871B710C2CC98073748424/administrators/HistoryList?ID=@Model.ID" data-key="4">
                                <thead>
                                    <tr>
                                        <th>IP Address</th>
                                        <th>User Agent</th>
                                        <th>Start Time</th>
                                        <th>End Time</th>
                                        <th class="no-sort">Duration</th>
                                    </tr>
                                </thead>
                                <tbody></tbody>
                            </table>
                        </div>

                    </div>

                </div>
            </div>
        </div>
    </div>

</div>

