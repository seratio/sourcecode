@{
    Layout = null;
}

<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Sign In - Seratio Platform</title>
    <link rel="shortcut icon" href="/areas/users/assets/images/favicon.png">
    <link href="/areas/users/assets/css/bootstrap.min.css" rel="stylesheet" type="text/css" />
    <link href="/areas/users/assets/css/core.css" rel="stylesheet" type="text/css" />
    <link href="/areas/users/assets/css/components.css" rel="stylesheet" type="text/css" />
    <link href="/areas/users/assets/css/icons.css" rel="stylesheet" type="text/css" />
    <link href="/areas/users/assets/css/pages.css" rel="stylesheet" type="text/css" />
    <link href="/areas/users/assets/css/responsive.css" rel="stylesheet" type="text/css" />
    <link href="/areas/users/assets/css/custom.css?_=@DateTime.Now.Ticks" rel="stylesheet" type="text/css" />
    <link href="/areas/users/assets/plugins/bootstrap-toastr/toastr.min.css" rel="stylesheet" type="text/css" />
    <link rel="stylesheet" href="//cdnjs.cloudflare.com/ajax/libs/timepicker/1.3.5/jquery.timepicker.min.css" />

    <script src="/areas/users/assets/js/modernizr.min.js"></script>
</head>
<body class="pantone">
    <div class="account-pages @(Request.QueryString["type"] == "Womens Coin" ? " womens_coin_vector" : "")"></div>
    <div class="clearfix"></div>
    <div class="wrapper-page@(Request.QueryString["type"] == "Womens Coin" ? " womens_coin" : "")">
        <div class="card-box">
            <div class="panel-heading text-center">
                @{
                    if (Request.QueryString["type"] == "Womens Coin")
                    {
                        <img src="~/areas/users/assets/images/womenscoinlogov2.png" alt="Womens Coin" style="width: 160px;" />
                        <p style="font-size: 10px; text-align: justify; margin: 0;">CCEG is managing the Initial Coin Offering and this Private Placement Investment of Women’s Coin; this includes 5,000,000 Women’s Coin digital cryptocurrency tokens in the SER token, and Women’s Coin micro-share. Each SER token and its accompanying Women’s Coin micro-share (the "Tokens") are offered by Women’s Coin Ltd at a price of GBP £1.00. Minimum investment is £1,000 – the purchase of SER Token and Micro-shares are subject to terms and conditions available on <a href="https://seratio-coins.world/" target="_blank">www.seratio-coins.world</a>. All investors must fall within the definition of “Experienced Investor” which is contained in the Financial Services (Experienced Investor Funds) Regulations.</p>
                        <div style="background: #92278f; border-radius: 3px; text-align:center;color: #fff;margin: 10px 0;font-size: 18px;font-weight: 600;padding: 13px;">
                            BUY NOW PRE-SALE
                        </div>
                        <div style="background: #92278f; border-radius: 3px; text-align:center;color: #fff;margin: 10px 0 0;font-size: 14px;font-weight: 600;padding: 13px;">
                            REGISTER YOUR EMAIL AND PASSWORD TO PROCEED TO THE INVESTMENT PAGE
                        </div>
                    }
                    else if (Request.QueryString["type"] == "Student Coin")
                    {
                        <img src="~/areas/users/assets/images/studentcoin.png" alt="Womens Coin" style="width: 160px;" />
                    }
                    else if (Request.QueryString["type"] == "TimeTraveler Coin")
                    {
                        <img src="~/areas/users/assets/images/timetraveler.png" alt="TimeTraveler Coin" style="width: 160px;" />
                    }
                    else
                    {
                        <img src="~/areas/users/assets/images/logo.png" alt="Seratio Platform" style="width: 200px;" />
                    }
                }

            </div>
            <div class="panel-body">
                <form class="form-horizontal" action="#" method="post">
                    <div class="form-group">
                        <div class="col-xs-12">
                            <input class="form-control" type="text" required="" placeholder="Email" name="Email">
                        </div>
                    </div>
                    <div class="form-group">
                        <div class="col-xs-12">
                            @{
                                if (Request.QueryString["type"] == "Womens Coin")
                                {
                                    <input class="form-control change-input" type="password" autocomplete="off" required="" placeholder="Password" name="password">
                                    <i class="fa fa-eye show-password"></i>
                                }
                                else
                                {
                                    <input class="form-control" type="password" required="" autocomplete="off" placeholder="Password" name="password">
                                }
                            }
                        </div>
                    </div>

                    <div class="form-group text-center m-t-20">
                        <div class="col-xs-12">
                            @{
                                if (Request.QueryString["type"] != "Womens Coin" && Request.QueryString["type"] != "Growth Token")
                                {
                                    <button class="btn btn-block text-uppercase waves-effect waves-light" style="background: #0168ab; color: #fff; font-weight: bold;" type="submit">@*Existing Clients*@ Sign In</button>
                                }
                                else if (Request.QueryString["type"] == "Growth Token")
                                {
                                    <button class="btn btn-block text-uppercase waves-effect waves-light" style="background: #0168ab; color: #fff; font-weight: bold;" type="submit">Existing Clients Sign In</button>

                                }
                                else
                                {
                                    <button class="btn btn-block text-uppercase waves-effect waves-light" style="background: #92278f !important; color: #fff; font-weight: bold;" type="submit">CLICK HERE TO ACCESS A SECURE INVESTMENT SITE</button>
                                }
                            }

                        </div>
                    </div>
                    <div class="form-group m-t-30 m-b-0">
                        <div class="col-sm-12">
                            <a href="/users/default/Forgot" class="text-dark"><i class="fa fa-lock m-r-5"></i> Forgot your password?</a>
                        </div>
                    </div>
                </form>
            </div>
        </div>
        <div class="row">
            <div class="col-sm-12 text-center">
                @{
                    if (Request.QueryString["type"] != "Growth Token")
                    {
                        if (Request.QueryString["type"] == "Student Coin" || Request.QueryString["type"] == "TimeTraveler Coin")
                        {
                            <p style="font-weight: bold;">Don't have a @(Request.QueryString["type"]) account? Simply <a href="/users/default/signup?type=@(Request.QueryString["type"])" style="border-radius: 0;">click here</a> to sign up for a new account via the Seratio Platform</p>
                        }
                        else if (string.IsNullOrEmpty(Request.QueryString["type"]))
                        {
                            <p style="font-weight: bold;">Don't have an account? Simply <a href="/users/default/signup" style="border-radius: 0;">click here</a> to sign up for a new Seratio Platform account.</p>
                        }
                        else if (!string.IsNullOrEmpty(Request.QueryString["type"]) && Request.QueryString["type"] != "Womens Coin" && Request.QueryString["type"] != "Student Coin")
                        {
                            <p style="font-weight: bold;">Don't have a @(Request.QueryString["type"]) account? Simply click the logo below to sign up for a new account via the Seratio Platform</p>
                        }
                        else if (Request.QueryString["type"] != "Womens Coin")
                        {
                            <p style="font-weight: bold;">Don't have an account? Simply click the logo below to sign up for a new account via the Seratio Platform</p>
                        }
                    }

                    if (Request.QueryString["type"] == "Womens Coin" || Request.QueryString["type"] == "Student Coin" || Request.QueryString["type"] == "TimeTraveler Coin")
                    {
                        <img src="~/areas/users/assets/images/logo.png" style="width: 100px;" />
                    }
                    else if (Request.QueryString["type"] == "Growth Token")
                    {
                        @*<a href="/users/default/signup?type=Growth Token" style="border-radius: 0;">*@
                            <img src="~/areas/users/assets/images/10xaccountlogo.png" style="width: 200px;" />
                        @*</a>*@
                    }
                    else if (!string.IsNullOrEmpty(Request.QueryString["type"]))
                    {
                        <a href="/users/default/signup?type=Womens Coin" style="border-radius: 0;">
                            <img src="~/areas/users/assets/images/womenscoinlogov2.png" style="width: 100px;" />
                        </a>
                        <a href="/users/default/signup?type=Student Coin" style="border-radius: 0;">
                            <img src="~/areas/users/assets/images/studentcoin.png" style="width: 100px;" />
                        </a>
                        <a href="/users/default/signup?type=Growth Token" style="border-radius: 0;">
                            <img src="~/areas/users/assets/images/10xaccountlogo.png" style="width: 200px;" />
                        </a>
                    }
                }
            </div>
        </div>
    </div>

    <script>
        var resizefunc = [];
    </script>
    <!-- jQuery  -->
    <script type="text/javascript" src="/areas/users/assets/js/jquery.min.js"></script>
    <script type="text/javascript" src="/areas/users/assets/js/bootstrap.min.js"></script>
    <script type="text/javascript" src="/areas/users/assets/js/moment.min.js"></script>
    <script type="text/javascript" src="~/areas/users/assets/js/select2.js"></script>
    <script type="text/javascript" src="/areas/users/assets/plugins/pickers/pickadate/picker.js"></script>
    <script type="text/javascript" src="/areas/users/assets/plugins/pickers/pickadate/picker.date.js"></script>
    <script type="text/javascript" src="/areas/users/assets/plugins/forms/styling/uniform.min.js"></script>
    <script type="text/javascript" src="https://cdn.datatables.net/1.10.9/js/jquery.dataTables.min.js"></script>
    <script src="~/areas/users/assets/js/app.js" type="text/javascript"></script>
    <script src="~/areas/users/assets/js/jquery.livequery.js" type="text/javascript"></script>
    <script src="~/Scripts/jquery.validate.min.js" type="text/javascript"></script>
    <script src="https://cdn.jsdelivr.net/jquery.validation/1.15.0/additional-methods.min.js"></script>
    <script src="~/areas/users/assets/plugins/bootstrap-toastr/toastr.min.js" type="text/javascript"></script>
    <script src="/areas/users/assets/plugins/bootstrap-daterangepicker/daterangepicker.js" type="text/javascript"></script>
    <script src="~/areas/users/assets/plugins/pickers/datepicker.js" type="text/javascript"></script>
    <script src="/areas/users/assets/js/jquery.cookie.min.js" type="text/javascript"></script>
    <script src="~/Scripts/jquery.validate.unobtrusive.min.js"></script>
    @*<script src="~/Scripts/html5-qrcode.min.js" type="text/javascript"></script>*@
    <script type="text/javascript" src="/areas/users/assets/js/scripts.js"></script>
    <script type="text/javascript" src="https://cdn.rawgit.com/bassjobsen/Bootstrap-3-Typeahead/master/bootstrap3-typeahead.min.js"></script>
    <script src="~/Scripts/app.js?_=@DateTime.Now.Ticks" type="text/javascript"></script>

    <script>
        $(document).ready(function () {
            App.init();
            @{
                            if (TempData["Notification"] != null)
                            {
                                Notification notification = (Notification)TempData["Notification"];
                    <text>WebApp.Notifications.Show('@notification.Type', '@notification.Text');</text>
                            }
                        }
        });
    </script>
</body>
</html>