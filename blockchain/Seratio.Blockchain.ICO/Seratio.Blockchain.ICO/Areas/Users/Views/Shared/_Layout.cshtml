                        {
                            string _icoType = Session["ICOType"].ToString();

                            if (_icoType == "Womens Coin")
                            {
                                <div class="logged-user-w">
                                    <img src="/areas/users/assets/images/womenscoinlogov2.png" style="width: 60%;">
                                </div>
                            }
                            else if (_icoType == "Student Coin")
                            {
                                <div class="logged-user-w">
                                    <img src="/areas/users/assets/images/studentcoin.png" style="width: 60%;">
                                </div>
                            }    
                            else if (_icoType == "Growth Token")
                            {
                                <div class="logged-user-w">
                                    <img src="/areas/users/assets/images/10xaccountlogo.png" style="width: 100%;">
                                </div>
                            }      
                            else if (_icoType == "TimeTraveler Coin")
                            {
                                <div class="logged-user-w">
                                    <img src="/Assets/images/timetraveler.png" style="width: 60%;">
                                </div>
                            } 
                        }
                        else
